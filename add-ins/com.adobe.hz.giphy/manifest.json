{
    "id": "giphy",
    "name": "Giphy",
    "version": "1.0.0",
    "manifestVersion": 1,
    "requirements": {
        "apps": ["Express"]
    },
    "icon": [
        {
            "width": 100,
            "height": 100,
            "path": "icon.png",
            "theme": ["all"]
        }
    ],
    "entryPoints": [
        {
            "type": "panel",
            "id": "giphy.entrypoint1",
            "main": "index.html",
            "label": {
                "default": "Giphy"
            }
        }
    ]
}
