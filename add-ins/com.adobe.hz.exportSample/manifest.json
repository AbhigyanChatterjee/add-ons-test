{
    "id": "export-sample",
    "name": "Export Sample",
    "version": "1.0.0",
    "manifestVersion": 1,
    "requirements": {
        "apps": [
            "Express"
        ]
    },
    "icon": [
        {
            "width": 100,
            "height": 100,
            "path": "icon1.png",
            "theme": [
                "all"
            ]
        }
    ],
    "entryPoints": [
        {
            "type": "panel",
            "id": "panel1",
            "main": "index.html",
            "label": {
                "default": "Export Sample"
            },
            "defaultSize": {
                "width": 600,
                "height": 525
            },
            "permissions": {
                "sandbox": ["allow-popups", "allow-presentation", "allow-downloads"]
            }
        }
    ]
}
