    "icon": {
      "width": 200,
      "height": 300,
      "path": "YoutubeWidgetIcon.jpeg",
      "theme": ["all"]
    },
    "authorInfo": {
      "name": "Adobe Inc.",
      "email": "support@adobe.com"
    }
